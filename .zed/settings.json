{
	"auto_install_extensions": {
		"biome": true,
		"dockerfile": true,
		"html": true,
		"nix": true,
		"terraform": true,
		"toml": true,
		"vue": true
	},
	"agent": {
		"model_parameters": [],
		"default_model": {
			"provider": "openai",
			"model": "anthropic/claude-3.5-sonnet"
		},
		"version": "2"
	},
	"base_keymap": "VSCode",
	"theme": "Ayu Dark",
	"telemetry": {
		"diagnostics": false,
		"metrics": false
	},
	"vim_mode": false,
	"ui_font_size": 13,
	"preferred_line_length": 120,
	"show_whitespaces": "none",
	"remove_trailing_whitespace_on_save": false,
	"soft_wrap": "editor_width",
	"ui_font_family": "IBM Plex Mono",
	"buffer_font_size": 13,
	"buffer_font_family": "IBM Plex Mono",
	"hard_tabs": true,
	"tab_size": 3,
	"formatter": "auto",
	"format_on_save": "on",
	"code_actions_on_format": {
		"source.fixAll": true,
		"source.organizeImports": true
	},
	"git": {
		"inline_blame": {
			"enabled": true,
			"min_column": 60,
			"show_commit_summary": true
		}
	},
	"language_models": {
		"openai": {
			"version": "1",
			"api_url": "https://openrouter.ai/api/v1",
			"available_models": [
				{
					"display_name": "OpenRouter Claude 3.5 Sonnet",
					"name": "anthropic/claude-3.5-sonnet",
					"max_tokens": 200000
				},
				{
					"display_name": "OpenRouter Claude 3.7 Sonnet",
					"name": "anthropic/claude-3.7-sonnet",
					"max_tokens": 200000
				},
				{
					"display_name": "OpenRouter GPT-4o-mini",
					"name": "openai/gpt-4o-mini",
					"max_tokens": 128000
				},
				{
					"display_name": "OpenRouter Gemini 2.0 Flash",
					"name": "google/gemini-2.0-flash-001",
					"max_tokens": 1048576
				},
				{
					"display_name": "OpenRouter Gemini 2.5 Pro Preview",
					"name": "google/gemini-2.5-pro-preview",
					"max_tokens": 1048576
				}
			]
		}
	}
}
